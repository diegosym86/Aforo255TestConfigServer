spring.application.name=ms-transaction
server.port=8082

#Conexión a Mongo
#spring.data.mongodb.uri=mongodb://mongodb:27017/db_account
spring.data.mongodb.uri=mongodb://localhost:27017/db_transaction

#Conexión a kafka 
#spring.kafka.consumer.bootstrap-servers=servicekafka:9092
spring.kafka.consumer.bootstrap-servers=localhost:9092
spring.kafka.consumer.key-deserializer=org.apache.kafka.common.serialization.IntegerDeserializer
spring.kafka.consumer.value-deserializer=org.apache.kafka.common.serialization.StringDeserializer
spring.kafka.consumer.group-id= transaction-events-transaction